    "glob": "^7.1.3",
    "helmet": "^3.16.0",
    "jsonwebtoken": "^8.5.0",
    "lock": "^1.1.0",
    "mariadb": "^2.0.3",
    "module-alias": "^2.2.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "nodemailer": "^6.4.2",
