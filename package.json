    "@types/cors": "^2.8.4",
    "@types/doctrine": "^0.0.3",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/faker": "^4.1.5",
    "@types/glob": "^7.1.1",
    "@types/helmet": "^0.0.43",
